#include <stdio.h>
#include <stdlib.h>

int main()
{
    int col,suma,rand,i,j,A[50][50],tab[50][50];
    printf("Randuri= ");
scanf("%d", &rand);

  for(i = 0; i < rand;i++){
 for(j = 0; j < rand; j++){
 printf("A[%d][%d]=", i,j);
 scanf("%d", &A[i][j]);}}
 for(int i=0;i<rand;i++)
        for(int j=0;j<rand;j++)
        {
            if(i==j)
            {
                int aux;
                aux=A[i][j];
                A[i][j]=A[i][rand-j-1];
                A[i][rand-j-1]=aux;
            }
        }
   for(int i=0;i<rand;i++)
        for(int j=0;j<rand;j++)
        printf("%2d", A[i][j]);
    return 0;
}
