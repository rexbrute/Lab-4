#include <stdio.h>
#include <stdlib.h>
int main()
{
    int rand,i,j,B[50][50],C[50][50],A[50][50];
    printf("Randuri= ");
    scanf("%d", &rand);

    for(i = 0; i < rand; i++)
    {
        for(j = 0; j < rand; j++)
        {
            printf("A[%d][%d]=", i,j);
            scanf("%d", &A[i][j]);
        }
    }
    for(i=0,j=0; i< rand; i++,j++){
        B[j][0]=A[i][j];}
    for(i=rand,j=0; i=0; i--,j++){
        C[j][0]=A[i][j];}
    for(i=rand,j=0; i=0; i--,j++){
        A[i][j]=B[j][0];}
    for(i=0,j=0; i< rand; i++,j++){
        A[i][j]=C[j][0];}
        printf("%d\n", C[0][0]);
    for(i = 0; i < rand; i++){
        for(j = 0; j < rand; j++){
            printf("%3d", A[i][j]);}}
    return 0;
}
